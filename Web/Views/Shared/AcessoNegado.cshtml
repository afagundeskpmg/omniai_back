@{
    Layout = "";
}

<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Assets/images/favicon.ico">
    <title>KPMG Watch - Portal Third Party Risk Management</title>
    <!-- Custom CSS -->

    <link href="~/assets/template/style.min.css" rel="stylesheet" />
    <script src="~/assets/js/shared.js"></script>
</head>
<body>

    <div class="error-box">
        <div class="error-body text-center">
            <img src="~/assets/images/logo/logo-transp-azul.png" width="150" />
            <div class="mt-3">
                <h3>@Multilingua.AlertasNotificacoes.AlertasNotificacoes.AcessoNegado</h3>
                <h5 class="m-b-0 text-muted font-medium" style="padding-top:10vh">A página que você solicitou não pôde ser acessada</h5>
            </div>
            <div class="mt-3">
                <button class="btn btn-info" onclick="Redirecionar('@Url.Action("Index","Home")')">Voltar</button>
            </div>
        </div>
    </div>
</body>
</html>