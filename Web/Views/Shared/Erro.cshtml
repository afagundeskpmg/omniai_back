@{
    if (!User.Identity.IsAuthenticated)
    {
        Layout = "";

        <link href="~/assets/template/style.min.css" rel="stylesheet" />
    }
}

<div class="container-fluid">
    <div class="card">
        <div class="card-body error-body text-center">
            <img src="~/assets/images/logo/logo-transp-azul.png" width="150" />
            <h2 class="text-dark pt-4">Watch</h2>
            <div class="mt-3">
                <h5 class="m-b-0 text-muted font-medium">Tivemos um problema ao processar a sua solicitação</h5>
                <h5 class="text-muted font-medium">Por favor, tente novamente mais tarde</h5>
                <span class="card-subtitle">@ViewBag.Erro</span>
            </div>
            <div class="mt-3"><i class="ti-settings font-24"></i></div>
        </div>
    </div>
</div>


